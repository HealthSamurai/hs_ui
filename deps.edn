{:paths ["src"]
 :deps  {re-frame/re-frame {:mvn/version "1.4.3"}
         reagent/reagent   {:mvn/version "1.2.0"}}

 :deps/prep-lib
 {:alias :prep
  :fn prep/pre-run-time-preparations!
  :ensure "resources"}

 :aliases
 {:shadow
  {:extra-deps {thheller/shadow-cljs {:mvn/version "2.28.11"}}
   :main-opts  ["-m" "shadow.cljs.devtools.cli"]}

  :prep
  {:extra-paths ["scripts"]}}}
